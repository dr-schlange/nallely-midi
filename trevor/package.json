{
	"name": "trevor",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"format": "yarn format-trevor && yarn format-jslib",
		"format-trevor": "biome format src --fix",
		"format-jslib": "biome format src --fix ../libs/js",
		"preview": "vite preview",
		"deploy:gh": "GH=true yarn build && gh-pages -d dist",
		"deploy:neutralino": "yarn build && cp -r dist/* ../trevor-neutralino/resources && cd ../trevor-neutralino && neu build"
	},
	"dependencies": {
		"@codemirror/autocomplete": "^6.20.0",
		"@codemirror/lang-python": "^6.2.1",
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/modifiers": "^9.0.0",
		"@dnd-kit/sortable": "^10.0.0",
		"@dnd-kit/utilities": "^3.2.2",
		"@reduxjs/toolkit": "^2.11.2",
		"@uiw/react-codemirror": "^4.25.4",
		"ansi_up": "^6.0.6",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"react-force-graph-3d": "^1.29.0",
		"react-redux": "^9.2.0",
		"react-uplot": "^0.0.9",
		"three-spritetext": "^1.10.0",
		"uplot": "^1.6.32",
		"uplot-react": "^1.2.2"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.10",
		"@eslint/js": "^9.39.2",
		"@types/node": "^22.15.21",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^4.4.1",
		"gh-pages": "^6.3.0",
		"globals": "^16.5.0",
		"typescript": "~5.9.3",
		"typescript-eslint": "^8.50.1",
		"vite": "^6.3.5"
	}
}
